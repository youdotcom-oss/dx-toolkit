{
  "$schema": "https://biomejs.dev/schemas/2.3.8/schema.json",
  "extends": "//",
  "overrides": [
    {
      "includes": ["examples/**/*.ts", "src/tests/**/*.spec.ts"],
      "linter": {
        "rules": {
          "suspicious": {
            "noConsole": "off",
            "noExplicitAny": "off"
          }
        }
      }
    },
    {
      "includes": ["src/client.ts"],
      "linter": {
        "rules": {
          "suspicious": {
            "noConsole": {
              "level": "off"
            }
          }
        }
      }
    }
  ]
}
